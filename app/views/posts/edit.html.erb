  <div class="field">
    <%= form_for @post do |f| %>
    <%= f.label :演奏会名 %>
    <br><%= f.text_field :name1 %>
  </div>

  
  <div class="field">
    <%= f.label :団体名 %>
    <br><%= f.text_field :name2 %>
  </div>

  <div class="field">
    <%= f.label :自由記述 %>
    <br><%= f.text_area :detail %>
  </div>

  <div class="field">
    <%= f.label :曲目 %>
    <br><%= f.text_area :music %>
  </div>

  <div class="field">
    <%= f.label :開催地域（都道府県） %>
    <br><%= f.text_field :prefecture %>
  </div>

  <div class="field">
    <%= f.label :チラシ画像（表） %>
