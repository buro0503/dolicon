
<div class="posts-container">
<% @posts.each do |t| %>
  <div class="post">
  <%= t.created_at %>
  <%= link_to "詳細へ", post_path(t.id) %><br>
  <%= t.name1 %> <br>
  <%= t.name2 %> <br>
  <%= t.date %><br>
  <%= t.start_time %><br>
  <%= t.detail %> <br>
  <%= t.music %> <br>
  <%= t.prefecture %><br> 
  <%= t.site %><br>
end